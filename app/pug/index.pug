include ../blocks.project/includes

doctype html
html(lang='ru')
  include templates/head/head
    block head
      - var pageTitle = 'Store Gift - магазин хороших подарков';

  +page._with-catalog

    +header(true, true).__header


    +b.content._with-catalog.__content

      +catalog.__catalog

      +e.inner
